page {
  height: 100%;
  background-color: #F5F5F5;
}

.enter-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 100%;
  background-color: #F5F5F5;
}

.toolarea{
  display: flex;
  width: 100%;
  height: 58px;
  margin-top: 3px;
  justify-content: space-between;
  align-items: center;
}
.tool{
  height: 100%;
  width: 21%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.image-tool{
  width: 24px;
  height: 24px;
}

.text-tool{
  font-size: 10px;
  line-height: 10px;
  font-family: SimSun;
  color: #101010;
}

.box-list .box-title-row{
  display: flex;
  justify-content: space-around;
  font-size: 15px;
  line-height: 42px;
  background-color: white;
  font-weight: 600;
  font-family: SimSun;
}
.box-list .box-data-row{
  display: flex;
  font-size: 12px;
  line-height: 42px;
  border-bottom: 1px solid #F2F2F2;
}
.box-list .box-column1{
  text-align: center;
  width: 31px;

}
.box-list .box-column2{
  text-align: center;

}
.box-list .box-column3{
  text-align: center;

}